{
  "name": "upload-oss-sample",
  "displayName": "Upload OSS Sample",
  "description": "",
  "publisher": "opensumi",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.36.0",
    "sumi": "^1.8.30"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*",
    "onCommand:HelloKaitian"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "HelloKaitian",
        "title": "HelloKaitian"
      }
    ]
  },
  "kaitianContributes": {
    "browserMain": "./out/browser/index.js",
    "nodeMain": "./out/node/index.js",
    "viewsProxies": [
      "UploadOss"
    ],
    "browserViews": {
      "left": {
        "type": "add",
        "view": [
          {
            "id": "UploadOss",
            "iconPath": "icons/oss.svg",
            "title": "Upload to OSS"
          }
        ]
      }
    }
  },
  "scripts": {
    "prepublishOnly": "npm run compile",
    "compile": "sumi compile",
    "watch": "sumi watch",
    "dev": "sumi dev -e=$(pwd)"
  },
  "devDependencies": {
    "@types/node": "^10.12.21",
    "@types/vscode": "^1.39.0",
    "@types/react": "^16.9.9",
    "tslint": "^5.12.1",
    "typescript": "^3.4.5",
    "vscode": "^1.1.28",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "@opensumi/sumi": "latest"
  },
  "dependencies": {
    "@opensumi/ide-components": "latest",
    "@opensumi/extension-logger": "^1.0.2",
    "@opensumi/antd-theme": "latest",
    "@types/ali-oss": "^6.0.7",
    "ali-oss": "^6.13.2",
    "antd": "^4.11.2"
  }
}
